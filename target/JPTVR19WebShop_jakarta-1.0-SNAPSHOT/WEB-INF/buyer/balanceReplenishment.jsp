<script>
    $('.creditCartText').on('keyup', function () {
        let foo = $(this).val().split(" ").join("");
        if (foo.length > 0) {
            foo = foo.match(new RegExp('.{1,4}', 'g')).join(" ");
        }
        $(this).val(foo);
    });

    $('.expDateCartText').on('keyup', function () {
        let foo = $(this).val().split(" / ").join("");
        if (foo.length > 0) {
            foo = foo.match(new RegExp('.{1,2}', 'g')).join(" / ");
        }
        $(this).val(foo);
    });
</script>